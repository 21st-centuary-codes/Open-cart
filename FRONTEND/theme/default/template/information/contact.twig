{{ header }}
<div class="container">
  <h1>{{ heading_title }}</h1>
  <form action="{{ action }}" method="post">
    <input type="text" name="name" placeholder="{{ entry_name }}">
    <input type="email" name="email" placeholder="{{ entry_email }}">
    <textarea name="enquiry" placeholder="{{ entry_enquiry }}"></textarea>
    <button type="submit" class="btn btn-primary">{{ button_submit }}</button>
  </form>
</div>
{{ footer }}
